     `beca_becario` beca_becario INNER JOIN `beca_datos_escolares` beca_datos_escolares ON beca_becario.`id_becario` = beca_datos_escolares.`id_becario`
     INNER JOIN `beca_cat_programa` beca_cat_programa ON beca_becario.`id_programa` = beca_cat_programa.`id_programa`
     INNER JOIN `beca_cat_estatus` beca_cat_estatus ON beca_becario.`id_estatus` = beca_cat_estatus.`id_estatus`
