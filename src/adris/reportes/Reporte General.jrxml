	<queryString>
		<![CDATA[SELECT
     beca_becario.`folio` AS beca_becario_folio,
     beca_becario.`ap_paterno` AS beca_becario_ap_paterno,
     beca_becario.`ap_materno` AS beca_becario_ap_materno,
     beca_becario.`nombre` AS beca_becario_nombre,
     beca_becario.`id_programa` AS beca_becario_id_programa,
     beca_becario.`id_estatus` AS beca_becario_id_estatus,
     beca_cat_programa.`id_programa` AS beca_cat_programa_id_programa,
     beca_cat_programa.`nombre` AS beca_cat_programa_nombre,
     beca_cat_estatus.`id_estatus` AS beca_cat_estatus_id_estatus,
     beca_cat_estatus.`nombre` AS beca_cat_estatus_nombre,
     beca_datos_escolares.`id_universidad` AS beca_datos_escolares_id_universidad,
     beca_cat_universidad_A.`id_universidad` AS beca_cat_universidad_A_id_universidad,
     beca_cat_universidad_A.`nombre` AS beca_cat_universidad_A_nombre,
     beca_datos_escolares.`id_campo_carrera` AS beca_datos_escolares_id_campo_carrera,
     beca_cat_campo.`id_campo_carrera` AS beca_cat_campo_id_campo_carrera,
     beca_cat_campo.`nombre` AS beca_cat_campo_nombre,
     beca_datos_escolares.`beca_total` AS beca_datos_escolares_beca_total,
     beca_datos_escolares.`beca_semestral` AS beca_datos_escolares_beca_semestral,
     beca_datos_escolares.`mes_inicio_beca` + 1 AS beca_datos_escolares_mes_inicio_beca,
     beca_datos_escolares.`anio_inicio_beca` AS beca_datos_escolares_anio_inicio_beca,
     beca_datos_escolares.`semestre_inicio_beca` AS beca_datos_escolares_semestre_inicio_beca,
     beca_datos_escolares.`mes_graduacion` AS beca_datos_escolares_mes_graduacion,
     beca_datos_escolares.`anio_graduacion` AS beca_datos_escolares_anio_graduacion
FROM
     `beca_cat_estatus` beca_cat_estatus INNER JOIN `beca_becario` beca_becario ON beca_cat_estatus.`id_estatus` = beca_becario.`id_estatus`
     INNER JOIN `beca_cat_programa` beca_cat_programa ON beca_becario.`id_programa` = beca_cat_programa.`id_programa`
     INNER JOIN `beca_datos_escolares` beca_datos_escolares ON beca_becario.`id_becario` = beca_datos_escolares.`id_becario`
     INNER JOIN `beca_cat_universidad` beca_cat_universidad ON beca_datos_escolares.`id_universidad` = beca_cat_universidad.`id_universidad`
     INNER JOIN `beca_cat_universidad` beca_cat_universidad_A ON beca_datos_escolares.`id_universidad` = beca_cat_universidad_A.`id_universidad`
     INNER JOIN `beca_cat_campo` beca_cat_campo ON beca_datos_escolares.`id_campo_carrera` = beca_cat_campo.`id_campo_carrera`]]>
	</queryString>
