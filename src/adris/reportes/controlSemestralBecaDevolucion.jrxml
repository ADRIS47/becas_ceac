	<queryString language="SQL">
		<![CDATA[SELECT
     beca_becario.`folio` AS beca_becario_folio,
     beca_becario.`ap_paterno` AS beca_becario_ap_paterno,
     beca_becario.`ap_materno` AS beca_becario_ap_materno,
     beca_becario.`nombre` AS beca_becario_nombre,
     beca_datos_escolares.`id_becario` AS beca_datos_escolares_id_becario,
     beca_datos_escolares.`id_universidad` AS beca_datos_escolares_id_universidad,
     beca_cat_universidad.`id_universidad` AS beca_cat_universidad_id_universidad,
     beca_cat_universidad.`nombre` AS beca_cat_universidad_nombre,
     beca_datos_escolares.`beca_semestral` AS beca_datos_escolares_beca_semestral,
     beca_datos_escolares.`fecha_inicio_beca` AS beca_datos_escolares_fecha_inicio_beca,
     beca_datos_escolares.`fecha_fin_beca` AS beca_datos_escolares_fecha_fin_beca,
     beca_kardex.`id_becario` AS beca_kardex_id_becario,
     beca_kardex.`pago_inicio_semestre` AS beca_kardex_pago_inicio_semestre,
     beca_kardex.`platica1` AS beca_kardex_platica1,
     beca_kardex.`platica2` AS beca_kardex_platica2,
     beca_kardex.`pago_fin_semestre` AS beca_kardex_pago_fin_semestre,
     beca_kardex.`id_servicio_comunitario` AS beca_kardex_id_servicio_comunitario,
     beca_cat_servicio_comunitario.`id_servicio_comunitario` AS beca_cat_servicio_comunitario_id_servicio_comunitario,
     beca_cat_servicio_comunitario.`nombre` AS beca_cat_servicio_comunitario_nombre,
     beca_kardex.`promedio` AS beca_kardex_promedio,
     beca_kardex.`horas_servicio` AS beca_kardex_horas_servicio
FROM
     `beca_becario` beca_becario INNER JOIN `beca_datos_escolares` beca_datos_escolares ON beca_becario.`id_becario` = beca_datos_escolares.`id_becario`
     INNER JOIN `beca_cat_universidad` beca_cat_universidad ON beca_datos_escolares.`id_universidad` = beca_cat_universidad.`id_universidad`
     INNER JOIN `beca_kardex` beca_kardex ON beca_becario.`id_becario` = beca_kardex.`id_becario`
     INNER JOIN `beca_cat_servicio_comunitario` beca_cat_servicio_comunitario ON beca_kardex.`id_servicio_comunitario` = beca_cat_servicio_comunitario.`id_servicio_comunitario`]]>
